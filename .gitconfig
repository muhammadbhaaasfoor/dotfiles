[user]
  name = Bryan Shell
  email = bryan.shell@orbitz.com

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch

  hist = log --graph --abbrev=7 --decorate -p

  l = log --graph --abbrev=7 --decorate
  ll = log --graph --stat --abbrev=7 --decorate

  lg = log --graph  --abbrev-commit --date=short --pretty=format:'%C(bold red)%h%Creset -%C(bold yellow)%d%Creset %s %C(bold green)(%cd) %C(bold blue)<%aN>%Creset'
  lga = log --all --graph --abbrev-commit --date=short --pretty=format:'%C(bold red)%h%Creset -%C(bold yellow)%d%Creset %s %C(bold green)(%cd) %C(bold blue)<%aN>%Creset'

  la = log --date=short --pretty=format:'%C(bold red)%h%Creset -%C(bold yellow)%d%Creset %s %C(bold green)(%cd) %C(bold blue)<%aN>%Creset'
  laa = log --date=short --all --pretty=format:'%C(bold red)%h%Creset -%C(bold yellow)%d%Creset %s %C(bold green)(%cd) %C(bold blue)<%aN>%Creset'

  type = cat-file -t
  dump = cat-file -p

  unpushed = !"PROJ_BRANCH=$(git symbolic-ref HEAD | sed 's|refs/heads/||') && git log origin/$PROJ_BRANCH..HEAD"
  unpulled = !"PROJ_BRANCH=$(git symbolic-ref HEAD | sed 's|refs/heads/||') && git fetch && git log HEAD..origin/$PROJ_BRANCH"

[color]
  ui = auto

[mergetool]
    prompt = false
